#: src/ui.cpp:259 src/ui.cpp:585
msgid "Software Update"
msgstr "עדכון תוכנה"

#: src/ui.cpp:436
msgid "Release notes:"
msgstr "הערות שחרור:"

#: src/ui.cpp:462
msgid "Skip this version"
msgstr "דלג על גרסה זו"

#: src/ui.cpp:468
msgid "Remind me later"
msgstr "הזכר לי מאוחר יותר"

#: src/ui.cpp:473 src/ui.cpp:486
msgid "Install update"
msgstr "התקן עדכון"

#: src/ui.cpp:584
#, c-format
msgid "%s cannot be restarted."
msgstr "לא ניתן להפעיל מחדש את %s."

#: src/ui.cpp:587
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "וודא שאין לך מסמכים לא שמורים ונסה שוב."

#: src/ui.cpp:594
msgid "Launching the installer..."
msgstr "מריץ את קובץ ההתקנה..."

#: src/ui.cpp:599
msgid "Failed to launch the installer."
msgstr "נכשל בהרצת קובץ ההתקנה."

#: src/ui.cpp:621
msgid "Checking for updates..."
msgstr "בודק אחר עדכונים..."

#: src/ui.cpp:623 src/ui.cpp:685 src/ui.cpp:765
msgid "Cancel"
msgstr "ביטול"

#: src/ui.cpp:641
msgid "You're up to date!"
msgstr "הכל מעודכן!"

#: src/ui.cpp:648
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s היא כרגע הגרסה הזמינה החדשה ביותר."

#: src/ui.cpp:661
msgid "Close"
msgstr "סגור"

#: src/ui.cpp:680
msgid "Update Error!"
msgstr "שגיאת עדכון!"

#: src/ui.cpp:682
msgid "An error occurred in retrieving update information. Are you connected to the internet? Please try again later."
msgstr "אירעה שגיאה באחזור פרטי עדכון; האם אתה מחובר.לאינטרנט? נסה שנית מאוחר יותר."

#: src/ui.cpp:723
#, c-format
msgid "A new version of %s is available!"
msgstr "גרסה חדשה של %s זמינה!"

#: src/ui.cpp:726
msgid "Get update"
msgstr "קבל עדכון"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:732
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s זמינה עכשיו (יש לך %s). האם ברצונך להוריד אותה כעת?"

#: src/ui.cpp:763
msgid "Downloading update..."
msgstr "מוריד עדכון..."

#: src/ui.cpp:791
#, c-format
msgid "%s of %s"
msgstr "%s מתוך %s"

#: src/ui.cpp:820
msgid "Ready to install."
msgstr "מוכן להתקנה."

#: src/ui.cpp:960
msgid "Check for updates automatically?"
msgstr "לבדוק אם קיימים עדכונים באופן אוטומטי?"

#: src/ui.cpp:970
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "האם %s תבדוק אם קיימים עדכונים באופן אוטומטי? ניתן תמיד לבדוק אחר עדכונים באופן ידני מהתפריט."

#: src/ui.cpp:984
msgid "Check automatically"
msgstr "בדוק באופן אוטומטי"

#: src/ui.cpp:989
msgid "Don't check"
msgstr "אל תבדוק"

